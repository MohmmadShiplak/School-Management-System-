<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

	<appSettings>
		<add key="ProjectName" value="School  Project" />

	

		<add key="Working Days" value="Sunday to Thursday" />


		<add key="holidays" value="Friday to Staturday " />


		<add key="SchoolOpeningTime" value="08:00 AM" />
		<add key="SchoolClosingTime" value="1:00 PM" />
	</appSettings>

	<connectionStrings>
		<add name="ConnectionString" connectionString="Server=.;Database=School Project;Integrated Security=True;" providerName="System.Data.SqlClient" />
	</connectionStrings>
	
	

</configuration>